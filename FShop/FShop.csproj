<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>WinExe</OutputType>
    <TargetFramework>net6.0-windows</TargetFramework>
    <Nullable>enable</Nullable>
    <UseWPF>true</UseWPF>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="ion_exit-outline.png" />
    <None Remove="ph_user-circle-light.png" />
    <None Remove="Resources\ion_exit-outline.png" />
    <None Remove="simple-line-icons_bag.png" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore.Sqlite" Version="7.0.3" />
    <PackageReference Include="Microsoft.EntityFrameworkCore.Tools" Version="7.0.3">
      <PrivateAssets>all</PrivateAssets>
      <IncludeAssets>runtime; build; native; contentfiles; analyzers; buildtransitive</IncludeAssets>
    </PackageReference>
  </ItemGroup>

  <ItemGroup>
    <Resource Include="Resources\ion_exit-outline.png" />
    <Resource Include="Resources\ph_user-circle-light.png" />
    <Resource Include="Resources\simple-line-icons_bag.png" />
  </ItemGroup>

</Project>
